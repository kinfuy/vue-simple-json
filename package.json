{
  "name": "vue-simple-json",
  "version": "1.0.8",
  "description": "Minimalist JSON editor",
  "main": "lib/index.js",
  "author": "Thezero",
  "license": "MIT",
  "private": false,
  "scripts": {
    "serve": "vite",
    "build": "gulp --require sucrase/register/ts --gulpfile build/gulpfile.ts",
    "clear": "rimraf dist",
    "update:version": "sucrase-node build/utils/version.ts",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly",
    "log": "npm run changelog",
    "changelog": "conventional-changelog -p custom-config  -i CHANGELOG.md  -s -r 0 -n ./changelog.config.js",
    "push": "git push origin master && git push githup master"
  },
  "keywords": [
    "json",
    "typescript",
    "vue3",
    "json-editor"
  ],
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.9",
    "@types/lodash.clonedeep": "^4.5.6",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vue/compiler-sfc": "^3.2.31",
    "archiver": "^5.3.1",
    "autoprefixer": "^10.4.2",
    "conventional-changelog-cli": "^2.0.34",
    "conventional-changelog-custom-config": "^0.3.1",
    "cz-conventional-changelog": "^3.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-less": "^5.0.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-esbuild": "^4.9.1",
    "stylelint": "^13.7.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "sucrase": "^3.21.0",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vue": "^3.2.16",
    "vue-tsc": "^0.29.8"
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-prettier"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}